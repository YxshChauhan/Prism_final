<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Airlink 4</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>airlink_4</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>NSLocalNetworkUsageDescription</key>
	<string>AirLink uses local network to discover and connect to nearby devices for file transfer.</string>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsLocalNetworking</key>
		<true/>
	</dict>
	<key>NSBluetoothAlwaysUsageDescription</key>
	<string>AirLink uses Bluetooth to discover and connect to nearby devices for file transfer.</string>
	<key>NSBluetoothPeripheralUsageDescription</key>
	<string>AirLink uses Bluetooth to discover and connect to nearby devices for file transfer.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>AirLink needs access to your photo library to send and receive photos.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>AirLink needs permission to add photos to your library during transfers.</string>
	<key>NSCameraUsageDescription</key>
	<string>AirLink needs access to your camera to send and receive photos.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>AirLink needs access to your microphone to send and receive audio files.</string>
	<key>NSContactsUsageDescription</key>
	<string>AirLink needs access to your contacts to enable phone replication features.</string>
	
	<!-- ==================== Background Processing ==================== -->
	<key>UIBackgroundModes</key>
	<array>
		<string>bluetooth-central</string>
		<string>bluetooth-peripheral</string>
		<string>processing</string>
		<string>background-fetch</string>
	</array>

	<!-- Bonjour services for local discovery -->
	<key>NSBonjourServices</key>
	<array>
		<string>_airlink._tcp</string>
	</array>

	<key>BGTaskSchedulerPermittedIdentifiers</key>
	<array>
		<string>com.airlink.transfer</string>
	</array>
	
	<!-- ==================== Required Device Capabilities ==================== -->
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>armv7</string>
		<string>bluetooth-le</string>
		<string>wifi</string>
	</array>
	
	<!-- ==================== Supported File Types ==================== -->
	<key>CFBundleDocumentTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeName</key>
			<string>All Files</string>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>LSHandlerRank</key>
			<string>Alternate</string>
			<key>LSItemContentTypes</key>
			<array>
				<string>public.item</string>
			</array>
		</dict>
	</array>
	
	<!-- ==================== URL Schemes ==================== -->
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>airlink</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>airlink</string>
			</array>
		</dict>
	</array>
</dict>
</plist>
